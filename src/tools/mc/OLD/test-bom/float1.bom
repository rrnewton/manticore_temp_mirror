(* float1.bom *)

module Float1

  extern void M_PrintFloat (float);

  fun init (u : unit/ exh : cont(any)) : int =
  
    fun retFloat (u : unit/ exh : cont(any)) : int =
	let f : float = 3.14
	let g : float = F32Add (f, f)
        do ccall M_PrintFloat (g)
        return (0)

    let x : int = apply retFloat (u / exh)
    return (x)
