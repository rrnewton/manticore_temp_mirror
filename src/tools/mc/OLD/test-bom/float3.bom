(* float3.bom *)

module Float3
  fun init (arg : any; exh : cont(any)) : unit =
    fun iterate (ar : (int, float); exh : cont(any)) : float =
	let i : int = #0(ar)
	let fl : float = #1(ar)
	case i
         of 0  => return (fl)
          | _  =>
              let f1 : float = 2.3
              let fl' : float = F32Add (fl, f1)
              return (fl')
	end
	
    return (enum(0))
