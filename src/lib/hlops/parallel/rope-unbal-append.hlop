#include "types.def"
#include "ropes.def"

extern void M_Print (void * );

define @rope-unbal-append (arg : [rope, rope] / exh : exh) : rope =

(* FIXME *)

  let r1 : rope = #0(arg)
  let r2 : rope = #1(arg)

  let r : rope = @simple-cat (r1, r2 / exh)
  return (r)
;
