#include "types.def"

extern int M_GetNumProcs ();

define inline @get-num-procs (_ : unit / exh : exh) : ml_int =
  let n : int = ccall M_GetNumProcs ()
  let wn : ml_int = wrap(n)
    return (wn)
;
