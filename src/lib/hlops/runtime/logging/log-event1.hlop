#include "logging.def"
#include "debugging.def"

extern void M_LogEvent1 (void*, int, int);

define inline @log-event1 (evt : log_event, aux : int) : () =
  do ccall M_LogEvent1 (host_vproc, evt, aux)
  return ()
;
