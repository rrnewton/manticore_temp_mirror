 * Get a double from stdin.
 *)

#include "types.def"

extern double M_ReadDouble ();

define @read-double (_ : unit / _ : exh) : ml_double =
    let y : double = ccall M_ReadDouble ()
    let res : ml_double = wrap(y)
      return (res)
;
